<div class="col-xs-10 col-xs-offset-1 col-md-4 col-md-offset-4 form-default">

  <%= simple_form_for pincode do |f| %>
  <%= f.error_notification %>
  <%= f.input :pin %>
  <%= f.input :city_id, collection: City.all %>
  <div class="hidden">
    <%= f.input_field "Pass Params", name: "company", value: params[:company] %>
  </div>
  <%= f.button :submit %>
  <% end %>

  <%= link_to "Go back", root_path %>
</div>
