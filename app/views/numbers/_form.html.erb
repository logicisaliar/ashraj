
<div class="col-xs-10 col-xs-offset-1 col-md-4 col-md-offset-4 form-default">
<% raise %>
  <%= simple_form_for number do |f| %>
  <%= f.error_notification %>
  <%= f.input :num, label: "Number" %>
  <%#= f.input :primary, label: "Type", collection: [:main, :landline, :other] %>
  <%= f.button :submit, "Submit", class: "btn-submit" %>
  <% end %>
</div>
